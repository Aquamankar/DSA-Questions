8. Reverse mapping Map<String, String> reverse it to Map<String,List<String>> .

import java.util.*;
import java.util.stream.Collectors;

class Main {
    public static void main(String[] args) {
        Map<String, List<String>> map = new HashMap<>();
        map.put("a", Arrays.asList("2", "4", "6"));
        map.put("b", Arrays.asList("2", "8"));

        Map<String, String> flattened = map.entrySet().stream()
            .flatMap(entry -> entry.getValue().stream()
                .map(value -> new AbstractMap.SimpleEntry<>(value, entry.getKey())))
            .collect(Collectors.toMap(
                Map.Entry::getKey,
                Map.Entry::getValue,
                (v1, v2) -> v1 // in case of duplicate key like "2", keep first (can change logic)
            ));

        System.out.println(flattened);
    }
}


//{2=a, 4=a, 6=a, 8=b}



---------------------------------------------------------

import java.util.Map;
import java.util.List;
import java.util.stream.Collectors;

public class MapReverser {

    public static void main(String[] args) {
        // Original Map: <Name, City>
        Map<String, String> originalMap = Map.of(
            "Alice", "New York",
            "Bob", "London",
            "Charlie", "Paris",
            "David", "London",  // Bob and David share the same city
            "Eve", "New York"   // Alice and Eve share the same city
        );

        // Reverse the map using Collectors.groupingBy()
        Map<String, List<String>> reversedMap = originalMap.entrySet().stream()
            // 1. Grouping Key: Use the original entry's VALUE (the City)
            .collect(Collectors.groupingBy(
                Map.Entry::getValue,
                // 2. Downstream Collector: Map the group members (Entries) 
                //    to their original KEY (the Name) and collect them into a List
                Collectors.mapping(
                    Map.Entry::getKey,
                    Collectors.toList()
                )
            ));

        System.out.println("Original Map: " + originalMap);
        System.out.println("------------------------------------------");
        System.out.println("Reversed Map (City -> List of Residents): " + reversedMap);
        
        // Expected Output:
        // Reversed Map (City -> List of Residents): {New York=[Alice, Eve], London=[Bob, David], Paris=[Charlie]}
    }
}



